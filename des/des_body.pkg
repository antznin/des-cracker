package body des_pkg is
  function left_shift (w:w28; amount : natural) return w28 is
  	   begin
	   if amount=2 then
	      return w28(28 to 3) & w28 (1 to 2);
	   elsif amount=1 then
	      return w28(2 to 28) & w28(1);
	   else
	      assert false report 'ERROR' severity failure;
	   end if;
  end left_shift;

end package body des_pkg;
